{
    "files.eol": "\n",
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.words": [
        "Popconfirm",
        "breadcrubm",
        "enter",
        "prettierrc",
        "stylelint"
    ]
}